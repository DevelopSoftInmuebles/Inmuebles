{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Inmueble list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tipo</th>
                <th>Direccion</th>
                <th>Estrato</th>
                <th>Agua</th>
                <th>Luz</th>
                <th>Gas</th>
                <th>Telefono</th>
                <th>Reparaciones</th>
                <th>Mejoras</th>
                <th>Novedades</th>
                <th>Inversiones</th>
                <th>Url</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('inmueble_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.direccion }}</td>
                <td>{{ entity.estrato }}</td>
                <td>{{ entity.agua }}</td>
                <td>{{ entity.luz }}</td>
                <td>{{ entity.gas }}</td>
                <td>{{ entity.telefono }}</td>
                <td>{{ entity.reparaciones }}</td>
                <td>{{ entity.mejoras }}</td>
                <td>{{ entity.novedades }}</td>
                <td>{{ entity.inversiones }}</td>
                <td>{{ entity.url }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('inmueble_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('inmueble_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('inmueble_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
